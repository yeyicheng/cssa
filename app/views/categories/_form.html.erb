<%= render 'shared/error_messages', :object => f.object %>
<div class="field">
	<%= f.label :name %><br />
	<%= f.text_field :name %>
</div>
<div class="field">
	<%= f.label :description %><br />
	<%= f.text_area :description, value: "Please enter descriptions", onFocus: "clearContents(this);" %>
</div>
<div class="field">
	<%= f.label :avatar %><br />
	<%= f.file_field :avatar %>
</div>
